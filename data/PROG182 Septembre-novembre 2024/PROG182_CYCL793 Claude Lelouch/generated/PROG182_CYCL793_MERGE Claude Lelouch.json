          ],
          "dureeSeance": 150,
          "mention": "Séance présentée par Daniel Vigne",
          "idCategorie": 3381,
          "idEvenement": 30886,
          "typeEvenement": 13,
          "idFilm": 49809,
          "art": "La",
          "titre": "Vie, l'amour, la mort",
          "realisateurs": "Claude Lelouch",
          "annee": 1969,
          "pays": "France-Italie",
          "duree": 115,
          "version": "",
