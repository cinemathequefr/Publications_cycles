          "idFilm": 155284,
          "titre": "Quai Sisowath",
          "realisateurs": "François Leroy, Stéphanie Lansaque",
          "annee": 2025,
          "pays": "France",
          "duree": 15,
          "version": "VOSTF",
          "format": "DCP",
          "textes": []
