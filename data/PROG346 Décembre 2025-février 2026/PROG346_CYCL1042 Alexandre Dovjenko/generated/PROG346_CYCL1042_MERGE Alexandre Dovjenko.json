          "typeEvenement": 13,
          "idFilm": 53567,
          "art": "La",
          "titre": "Terre",
          "titreVo": "Zemlia",
          "titreNx": "Земля",
          "realisateurs": "Alexandre Dovjenko",
          "annee": 1930,
          "pays": "URSS",
          "duree": 62,
          "version": "INT. FR.",
          "format": "DCP",
          "generique": [
