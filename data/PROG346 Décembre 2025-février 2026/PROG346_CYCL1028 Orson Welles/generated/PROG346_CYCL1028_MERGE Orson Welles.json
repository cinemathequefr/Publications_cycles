          "art": "La",
          "titre": "Nuit qui terrifia l'Amérique",
          "artVo": "The",
          "titreVo": "Night that Panicked America",
          "realisateurs": "Joseph Sargent",
          "annee": 1975,
          "pays": "États-Unis",
          "duree": 92,
          "version": "VOSTF",
          "format": "Numérique",
          "generique": [
            "Paul Shenar",
