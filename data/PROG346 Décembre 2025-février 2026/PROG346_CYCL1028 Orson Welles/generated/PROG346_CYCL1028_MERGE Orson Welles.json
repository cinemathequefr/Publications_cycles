          ],
          "dureeSeance": 165,
          "idCategorie": 3895,
          "idEvenement": 33963,
          "typeEvenement": 13,
          "idFilm": 146087,
          "titre": "Mank",
          "realisateurs": "David Fincher",
          "annee": 2020,
          "pays": "États-Unis",
          "duree": 131,
          "version": "VOSTF",
          "format": "DCP",
          "generique": [
            "Gary Oldman",
            "Amanda Seyfried",
            "Lily Collins"
          ],
          "textes": [
            {
              "typeTexte": 1000,
              "texte": "Tournée pour Netflix, sur un scénario du père de David Fincher, l'évocation du rôle ingrat des scénaristes dans le grand jeu de dupes hollywoodien – ici Herman J. Mankiewicz, auteur du script de _Citizen Kane_. Extraordinaire composition de Gary Oldman, reconstitution fastueuse, et constat un rien amer sur les coulisses de l'industrie des rêves."
            }
          ]
        },
        {
          "idSeance": 205627,
          "ordre": 1,
          "dateHeure": "2025-12-20T15:00:00",
          "idSalle": [
