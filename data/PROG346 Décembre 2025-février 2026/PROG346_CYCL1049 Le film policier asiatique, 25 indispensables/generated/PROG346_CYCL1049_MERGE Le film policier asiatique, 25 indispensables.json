          "typeEvenement": 13,
          "idFilm": 92666,
          "titre": "Infernal Affairs",
          "titreVo": "Wú jiān dào",
          "titreNx": "無間道",
          "realisateurs": "Andrew Lau, Alan Mak",
          "annee": 2002,
          "pays": "Chine",
