          "typeEvenement": 13,
          "idFilm": 97750,
          "titre": "PTU (Police Tactical Unit)",
          "titreVo": "Jǐngchá jīdòng bùduì",
          "titreNx": "警察機動部隊",
          "realisateurs": "Johnnie To",
