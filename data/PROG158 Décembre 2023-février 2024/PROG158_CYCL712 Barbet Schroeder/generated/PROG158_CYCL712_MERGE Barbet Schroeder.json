          "idFilm": 29146,
          "titre": "Before and After",
          "realisateurs": "Barbet Schroeder",
          "annee": 1995,
          "pays": "États-Unis",
          "duree": 108,
          "version": "VOSTF",
          "format": "35 mm",
          "generique": [
