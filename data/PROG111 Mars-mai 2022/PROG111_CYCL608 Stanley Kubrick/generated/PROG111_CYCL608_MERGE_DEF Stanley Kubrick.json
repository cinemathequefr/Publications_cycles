    {
      "titreSousCycle": "Rencontres et conférences",
      "tri": 4,
      "items": [
        {
          "idSeance": 66648,
          "ordre": 2,
          "dateHeure": "2022-05-07T15:00:00",
          "idSalle": [
            "HL",
