          "typeEvenement": 13,
          "idFilm": 41644,
          "art": "The",
          "titre": "Gamekeeper",
          "realisateurs": "Ken Loach",
          "annee": 1980,
          "pays": "Grande-Bretagne",
          "duree": 84,
          "version": "VO",
          "format": "35 mm",
          "generique": [
