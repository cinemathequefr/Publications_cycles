            "Jean Epstein"
          ],
          "dureeSeance": 120,
          "idCategorie": 3891,
          "idEvenement": 33771,
          "typeEvenement": 13,
          "idFilm": 120559,
          "titre": "Dreileben",
          "realisateurs": "Christian Petzold, Dominik Graf, Christoph Hochhäusler",
          "annee": 2011,
          "pays": "Allemagne",
          "duree": 88,
