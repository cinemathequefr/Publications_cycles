          "typeEvenement": 13,
          "idFilm": 33732,
          "titre": "Ya ya mon général",
          "titreVo": "Which Way to the Front ?",
          "realisateurs": "Jerry Lewis",
          "annee": 1969,
          "pays": "États-Unis",
          "duree": 96,
          "version": "",
          "format": "",
