          "idEvenement": 28297,
          "typeEvenement": 13,
          "idFilm": 101896,
          "titre": "Une vieille maîtresse",
          "realisateurs": "Catherine Breillat",
          "annee": 2006,
          "pays": "France-Italie",
          "duree": 114,
          "version": "",
          "format": "35 mm",
          "generique": [
