          "dateHeure": "2024-07-03T18:00:00",
          "idSalle": [
            "JE",
            "Jean Epstein"
          ],
          "dureeSeance": 135,
          "idCategorie": 3335,
          "idEvenement": 30772,
          "typeEvenement": 13,
          "idFilm": 105192,
          "titre": "Eldorado",
          "realisateurs": "Olivier Assayas",
